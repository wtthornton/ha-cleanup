          {
            "matcher": {
              "id": "byName",
              "options": "entity_id"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Automation Entity"
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "visible",
          "placement": "bottom",
          "showLegend": true
