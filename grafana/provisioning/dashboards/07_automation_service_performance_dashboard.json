        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> filter(fn: (r) => r[\"domain\"] == \"automation\") |> filter(fn: (r) => r[\"_field\"] == \"state\") |> group(columns: [\"entity_id\"]) |> count() |> sort(columns: [\"_value\"], desc: true) |> limit(n: 10) |> yield(name: \"Automation Performance\")",
          "refId": "A"
