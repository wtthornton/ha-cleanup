{
  "id": null,
  "title": "PROD: Automation & Service Performance Dashboard",
  "tags": ["automation", "service-performance", "reliability-monitoring", "home-assistant", "ha-ingestor", "production", "prod"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "time": {"from": "now-24h", "to": "now"},
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "templating": {
    "list": [
      {
        "name": "timeRange",
        "type": "interval",
        "query": "1h,6h,12h,24h,7d,30d",
        "queryValue": "24h",
        "current": {
          "selected": true,
          "text": "24h",
          "value": "24h"
        }
      },
      {
        "name": "serviceFilter",
        "type": "query",
        "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> filter(fn: (r) => r[\"domain\"] == \"service\") |> group(columns: [\"service\"]) |> distinct(column: \"service\") |> yield(name: \"Services\")",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Top Service Usage (Last 24h)",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> filter(fn: (r) => r[\"domain\"] == \"service\") |> filter(fn: (r) => r[\"_field\"] == \"state\") |> aggregateWindow(every: 2m, fn: count, createEmpty: false) |> group(columns: [\"service\"]) |> sum() |> sort(columns: [\"_value\"], desc: true) |> limit(n: 20) |> yield(name: \"Service Usage\")",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "auto",
            "align": "auto",
            "inspect": false
          },
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "service"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Service Name"
              },
              {
                "id": "custom.width",
                "value": 200
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "_value"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Usage Count"
              },
              {
                "id": "custom.align",
                "value": "right"
              },
              {
                "id": "unit",
                "value": "short"
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Usage Count"
          }
        ]
      }
    },
    {
      "id": 2,
      "title": "Automation Performance by Type (Last 24h)",
