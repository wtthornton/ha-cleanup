{
  "id": null,
  "title": "PROD: Raw Data Explorer Dashboard",
  "tags": ["raw-data", "data-exploration", "deep-analysis", "home-assistant", "ha-ingestor", "production", "prod"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "60s",
  "time": {"from": "now-24h", "to": "now"},
  "panels": [
    {
      "id": 1,
      "title": "Raw Event Data Table (Last 24h)",
      "type": "table",
      "gridPos": {"h": 12, "w": 24, "x": 0, "y": 0},
      "targets": [
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> sort(columns: [\"_time\"], desc: true) |> limit(n: 100) |> keep(columns: [\"_time\", \"_measurement\", \"entity_id\", \"domain\", \"state\", \"source\", \"_field\", \"_value\"]) |> yield(name: \"Raw Events\")",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "color-background"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "_time"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Timestamp"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "_value"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Count"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "entity_id"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Entity ID"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "domain"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Domain"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "state"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "State"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "source"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Source"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "_measurement"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Measurement"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "_field"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Field"
              }
            ]
          }
        ]
      }
    },
    {
      "id": 2,
      "title": "Data Distribution by Field Type (Last 24h)",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> group(columns: [\"_field\"]) |> count() |> yield(name: \"Field Types\")",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        },
        "pieType": "pie",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "id": 3,
      "title": "Entity State Distribution (Last 24h)",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "targets": [
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> filter(fn: (r) => r[\"_field\"] == \"state\") |> group(columns: [\"entity_id\", \"state\"]) |> count() |> sort(columns: [\"_value\"], desc: true) |> limit(n: 25) |> yield(name: \"State Distribution\")",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "color-background"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "_value"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Count"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "entity_id"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Entity ID"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "state"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "State"
              }
            ]
          }
        ]
      }
    },
    {
      "id": 4,
      "title": "Data Volume by Time (Last 24h)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 20},
      "targets": [
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> aggregateWindow(every: 1h, fn: count, createEmpty: false) |> yield(name: \"Data Volume\")",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "gradientMode": "scheme"
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 5,
      "title": "Top Data Sources (Last 24h)",
      "type": "barchart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
      "targets": [
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> group(columns: [\"source\"]) |> count() |> sort(columns: [\"_value\"], desc: true) |> limit(n: 15) |> yield(name: \"Source Activity\")",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "fillOpacity": 80
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Measurement Type Analysis (Last 24h)",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
      "targets": [
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> group(columns: [\"_measurement\"]) |> count() |> sort(columns: [\"_value\"], desc: true) |> limit(n: 20) |> yield(name: \"Measurement Types\")",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "color-background"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "_value"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Count"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "_measurement"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Measurement Type"
              }
            ]
          }
        ]
      }
    },
    {
      "id": 7,
      "title": "Data Quality Metrics (Last 24h)",
      "type": "stat",
      "gridPos": {"h": 4, "w": 24, "x": 0, "y": 36},
      "targets": [
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> filter(fn: (r) => r[\"_field\"] == \"state\") |> count() |> yield(name: \"State Changes\")",
          "refId": "A"
        },
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> filter(fn: (r) => r[\"_field\"] == \"value\") |> count() |> yield(name: \"Value Updates\")",
          "refId": "B"
        },
        {
          "query": "from(bucket: \"ha_events\") |> range(start: -24h) |> filter(fn: (r) => r[\"_measurement\"] =~ /ha_/) |> filter(fn: (r) => r[\"state\"] == \"unavailable\") |> count() |> yield(name: \"Unavailable Entities\")",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short",
          "custom": {
            "displayMode": "color-background"
          }
        }
      }
    }
  ]
}
